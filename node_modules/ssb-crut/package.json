{
  "name": "ssb-crut",
  "version": "6.1.1",
  "description": "easy CRUT methods for secure scuttlebutt",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "npm run test:js:db1 && npm run test:js:db2 && npm run lint && npm run test:only ",
    "test:js:db1": "tape 'test/**/*.test.js' | tap-arc --bail",
    "test:js:db2": "cross-env DB2=1 tape 'test/**/*.test.js' | tap-arc --bail",
    "test:only": "if grep -r --exclude-dir=node_modules --exclude-dir=.git --color 'test\\.only' ; then exit 1; fi",
    "lint": "standard --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/ahau/ssb-crut.git"
  },
  "keywords": [
    "scuttlebutt",
    "CRUT",
    "records"
  ],
  "author": "mixmix",
  "license": "LGPL-3.0-only",
  "bugs": {
    "url": "https://gitlab.com/ahau/ssb-crut/issues"
  },
  "homepage": "https://gitlab.com/ahau/ssb-crut#readme",
  "dependencies": {
    "@tangle/graph": "^3.2.0",
    "@tangle/reduce": "^5.0.5",
    "@tangle/strategy": "^4.1.2",
    "fast-json-stable-stringify": "^2.1.0",
    "is-my-ssb-valid": "^1.2.2",
    "lodash.merge": "^4.6.2",
    "pull-paramap": "^1.2.2",
    "pull-stream": "^3.7.0",
    "ssb-schema-definitions": "^3.2.1"
  },
  "devDependencies": {
    "@tangle/complex-set": "^3.0.3",
    "@tangle/linear-append": "^2.0.1",
    "@tangle/overwrite": "^3.0.1",
    "@tangle/simple-set": "^3.0.1",
    "cross-env": "^7.0.3",
    "scuttle-testbot": "^2.2.0",
    "ssb-backlinks": "^2.1.1",
    "ssb-box2": "^7.5.0",
    "ssb-classic": "^1.1.0",
    "ssb-db2": "^8.1.0",
    "ssb-query": "^2.4.5",
    "ssb-recps-guard": "^2.3.0",
    "ssb-tribes": "^4.0.0",
    "ssb-tribes-db1": "npm:ssb-tribes@3.1.3",
    "standard": "^17.1.0",
    "tap-arc": "^1.2.2",
    "tape": "^4.16.2"
  }
}
