{
  "name": "@tangle/strategy",
  "version": "4.1.2",
  "description": "a toolkit for composing tangle reducing strategies",
  "main": "index.js",
  "scripts": {
    "test": "npm run test:js && npm run lint && npm run test:only",
    "test:js": "tape test/*.test.js | tap-spec",
