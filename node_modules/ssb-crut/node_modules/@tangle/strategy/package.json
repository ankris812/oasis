    "dag"
  ],
  "author": "mixmix",
  "license": "LGPL-3.0-only",
  "devDependencies": {
    "@tangle/complex-set": "^3.0.1",
    "@tangle/overwrite": "^3.0.0",
    "@tangle/simple-set": "^3.0.0",
    "@tangle/test": "^2.1.0",
    "standard": "^16.0.3",
    "tap-spec": "^5.0.0",
    "tape": "^5.0.1"
  },
  "dependencies": {
    "is-my-json-valid": "^2.20.5",
    "lodash.isequal": "^4.5.0"
  }
}
